class Swappable&lt;T&gt; where T : class
{
    public T Active
    {
        get
        {
            return _swap ? _inst1 : _inst2;
        }
    }

    public T Inactive
    {
        get
        {
            return _swap ? _inst2 : _inst1;
        }
    }

    bool _swap = false;
    readonly T _inst1;
    readonly T _inst2;


    public Swappable(T pri, T sec)
    {
        _inst1 = pri;
        _inst2 = sec;
    }

    public void Swap()
    {
        _swap = !_swap;
    }
}

